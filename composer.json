{
    "name": "swapnilsarwe/laravel-flock-client",
    "description": "Laravel Package to access Flock Apis",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "SwapnilSarwe\\LaravelFlockClient\\": "src/",
            "SwapnilSarwe\\LaravelFlockClient\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SwapnilSarwe\\LaravelFlockClient\\FlockAppServiceProvider"
            ],
            "aliases": {
                "LaravelFlockApp": "SwapnilSarwe\\LaravelFlockClient\\Facades\\FlockAppService"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Swapnil Sarwe",
            "email": "swapnilsarwe@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
